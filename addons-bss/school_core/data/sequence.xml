<odoo>
    <data noupdate="1">
        <record id="seq_school_student" model="ir.sequence">
            <field name="name">Student ID</field>
            <field name="code">school.student</field>
            <field name="prefix">STU</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_teacher_employee_code" model="ir.sequence">
            <field name="name">Teacher Employee Code</field>
            <field name="code">school.teacher</field>
            <field name="prefix">TCH</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>
